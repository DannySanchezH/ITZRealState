@model IEnumerable<ITZRealStateWeb.Models.Room>
<link rel="stylesheet" type="text/css" href="~/Content/logs.css" />
<p>
    <input class="btn" type="button" onclick="CreateRoom();;" value="Create New"/>

</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.type)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.type)
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { id=item.IdRoom })
        </td>
    </tr>
}

</table>

@Scripts.Render("~/Scripts/Room/Index.js")
