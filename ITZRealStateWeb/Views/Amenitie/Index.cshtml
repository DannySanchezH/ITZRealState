@model IEnumerable<ITZRealStateWeb.Models.Amenitie>

<p>
     <a href="#" onclick="CreateAmenitie();">Create New</a>
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { id=item.IdAmenitie })
        </td>
    </tr>
}

</table>

@Scripts.Render("~/Scripts/Amenitie/Index.js")
